	otherbtmtop = $(".other_btm").offset().top;
});


var con1top = $(".modular_left .con1").offset().top;
var conrighttop = $(".modular_right .modular_main").offset().top;
var con1height = $(".modular_left .con1").height();
$(window).scroll(function(){
	var w_top=$(window).scrollTop();
	if(w_top<(otherbtmtop-320) && w_top>(con1top+con1height)){
		$(".modular_left .con2").show();
	}else{
		$(".modular_left .con2").hide();
	}
	if(w_top<(otherbtmtop-150) && w_top>(conrighttop-23)){
		$(".modular_right .conright").addClass("cur");
	}else{
		$(".modular_right .conright").removeClass("cur");
	}
});

var nowwidth = document.documentElement.clientWidth;
if(nowwidth<1200){
	$(".modular_left .con2").css({"left":0,"marginLeft":0});
}

$(".modular_left .con2 .con2p3").click(function(){
	$("html,body").animate({"scrollTop":0},500);
});

$(".modular_right .conright .conrightlist li").click(function(){
	var con2ind = $(this).index();
	var con3top = $(".modular_right .spjclist").eq(con2ind).offset().top;
	$("html,body").animate({
		"scrollTop":(con3top-91)
	},500);
});
$(".modular_right .box_con3 a").click(function(){
	var con3ind = $(this).index();
	$(this).addClass("a_on").siblings().removeClass("a_on");
	$(this).parents(".spjclist").find(".modular_listmain ul").eq(con3ind).show().siblings().hide();
});

